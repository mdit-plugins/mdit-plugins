  },
  "dependencies": {
    "@mdit/plugin-container": "workspace:*",
    "@types/markdown-it": "^14.1.2"
  },
  "devDependencies": {
    "markdown-it": "14.1.0"
  },
  "peerDependencies": {
    "markdown-it": "^14.1.0"
  },
  "peerDependenciesMeta": {
    "markdown-it": {
      "optional": true
    }
