{
  "name": "@mdit/plugin-emoji",
  "version": "0.2.1",
  "description": "emoji plugin for MarkdownIt",
  "keywords": [
    "emoji",
    "emojis",
    "emoticon",
    "emoticons",
    "markdown",
    "markdown-it",
    "markdown-it-plugin"
  ],
  "homepage": "https://github.com/mdit-plugins/mdit-plugins",
  "bugs": {
    "url": "https://github.com/mdit-plugins/mdit-plugins/issues"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdit-plugins/mdit-plugins.git",
    "directory": "packages/emoji"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "unpkg": "./dist/cdn.umd.js",
  "jsdelivr": "./dist/cdn.umd.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown --config-loader unrun",
    "clean": "rimraf ./dist",
    "update-emoji": "node --experimental-strip-types scripts/update.ts"
  },
  "dependencies": {
    "@types/markdown-it": "^14.1.2"
  },
  "peerDependencies": {
    "markdown-it": "^14.1.0"
  },
  "peerDependenciesMeta": {
    "markdown-it": {
      "optional": true
    }
  },
  "engines": {
    "node": ">= 20"
  }
}
